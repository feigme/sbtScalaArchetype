//文件用来声明当前项目希望使用哪些插件来增强当前项目使用的sbt的功能//本地仓库resolvers += "Local Maven Repository" at "file://D:/.m2/repository"// idea插件resolvers += "sbt-idea-repo" at "http://mpeltonen.github.com/maven/"addSbtPlugin("com.github.mpeltonen" % "sbt-idea" % "1.1.0")// 检查依赖关系resolvers += Resolver.url("git://github.com/jrudolph/sbt-dependency-graph.git")addSbtPlugin("net.virtual-void" % "sbt-dependency-graph" % "0.6.0")//